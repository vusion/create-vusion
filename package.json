{
  "name": "create-vusion",
  "version": "0.5.2",
  "description": "CLI for create Vusion Projects.",
  "main": "index.js",
  "bin": {
    "create-vusion": "bin/create-vusion"
  },
  "scripts": {
    "lint": "npx eslint --quiet",
    "lint:fix": "npm run lint -- \"./{lib,command,bin}/**/*.{js,vue}\" --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,vue}": [
      "npm run lint -- --fix",
      "git add"
    ]
  },
  "repository": "vusion/create-vusion",
  "keywords": [
    "vusion",
    "cli",
    "init"
  ],
  "tags": [
    "vusion",
    "cli",
    "init"
  ],
  "author": "",
  "license": "MIT",
  "bugs": "https://github.com/vusion/create-vusion/issues",
  "homepage": "https://github.com/vusion/create-vusion#readme",
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "@vusion/categories-db": "latest",
    "axios": "^0.19.0",
    "chalk": "^2.4.2",
    "commander": "^2.20.0",
    "glob": "^7.1.6",
    "inquirer": "^7.0.0",
    "ora": "^4.0.3",
    "request-pre": "^0.1.0",
    "semver": "^6.3.0",
    "shelljs": "^0.8.3",
    "update-notifier": "^3.0.1",
    "vusion-api": "^0.6.10"
  },
  "devDependencies": {
    "eslint": "^6.6.0",
    "eslint-config-vusion": "^3.0.2",
    "husky": "^3.0.9",
    "lint-staged": "^9.5.0"
  }
}
